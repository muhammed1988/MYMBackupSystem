[Unit]
Description=MYM Backup Server API
After=network.target

[Service]
WorkingDirectory=/var/www/mymbackup
ExecStart=/usr/bin/dotnet BackupServerApi.dll
Restart=always
RestartSec=10
SyslogIdentifier=mymbackup
User=www-data
Environment=ASPNETCORE_ENVIRONMENT=Production

[Install]
WantedBy=multi-user.target
